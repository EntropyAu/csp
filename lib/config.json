{
  "allHeaders": [
    "X-Content-Security-Policy",
    "Content-Security-Policy",
    "X-WebKit-CSP"
  ],
  "supportedDirectives": [
    "baseUri",
    "childSrc",
    "connectSrc",
    "defaultSrc",
    "fontSrc",
    "formAction",
    "frameAncestors",
    "frameSrc",
    "imgSrc",
    "manifestSrc",
    "mediaSrc",
    "objectSrc",
    "pluginTypes",
    "referrer",
    "reflectedXss",
    "reportUri",
    "sandbox",
    "scriptSrc",
    "styleSrc",
    "upgradeInsecureRequests"
  ],
  "mustBeQuoted": [
    "none",
    "self",
    "unsafe-inline",
    "unsafe-eval"
  ]
}
